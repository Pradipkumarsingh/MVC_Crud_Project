@model  IEnumerable<MVC_Crud_Project.Models.EmployeeDataModel>
@{
}
<a asp-action="Create" asp-controller="Employee" class="btn btn-danger">Create</a>
<table class="table table-bordered">
    <tr>
       
        <th>Name</th>
        <th>Mobile</th>
        <th>Email</th>
        <th>Country</th>
        <th>State</th>
        <th>City</th>
        <th>DOB</th>
    </tr>
    @foreach(var emp in Model)
    {

        //var s = Convert.ToString(emp.DOB);
        var s = String.Format("{0:dd/MM/yyyy}", emp.DOB);
<tr>
    <td>@emp.Name</td>
    <td>@emp.Mobile</td>
    <td>@emp.Email</td>
    <td>@emp.CountryName</td>
    <td>@emp.StateName</td>
    <td>@emp.CityName</td>
    <td>@s</td>
    <td><span><a asp-action="Update" asp-controller="Employee" asp-route-id="@emp.Id" class ="btn btn-danger"><u>Edit</u></a></span>
    <span>
        <a asp-action="Delete" asp-controller="Employee" asp-route-id="@emp.Id"  class ="btn btn-danger"
           onclick="return confirm('Are you sure you want to delete this data ?')">Delete</a>
       
    </span>
    </td>
</tr>
 }
   
</table>